@model UFPCore.Models.AdminPageViewModel
@{
    /**/

    ViewBag.Title = "Admin";
}

<h2>Admin Page</h2>

@Html.ActionLink("Refresh users in league to include all registered users", "InsertUsersToPointsTable", "Admin")

<hr />

<form action="/Admin/RemoveUserFromPointsTable" method="post">
    <div class="form-group">
        <label name="userName">
            User (UFPId) to remove from league
        </label>
        <input type="text" class="form-control" name="userName" />
    </div>
    <button class="btn btn-primary">Remove user from league </button>
</form>

<hr />

@Html.ActionLink("Update all scores (warning: can take some time, be patient)", "UpdateUserScores", "Admin")

<hr />

<form action="/Admin/AddOneToUpdateNumbers" method="post">
    <p>
        Select the number where you want to insert the new update. E.g. update 9 is completed and you want to move anything in update 10 to update 11, update 11 to update 12 etc. so that the unplanned result can be update 10, then select 10 here.
    </p>
    <div class="form-group">
        @Html.DropDownListFor(m => m.updateBreakPoint, Enumerable.Range(1, 60).Select(i => new SelectListItem { Text = i.ToString(), Value = i.ToString() }), "update number", new { @class = "form-control margine-left"})
    </div>
    <button class="btn btn-primary" type="submit"> Shift updates </button>
</form>

<hr />
